@model List<CarServiceModel>
@{
    ViewBag.Tittle = "All Cars For Sell";
}

<h2 class="text-center">@ViewBag.Tittle</h2>

@{
    if (Model.Count == 0)
    {
        <h3 class="text-center">There is no cars</h3>
    }
    else
    {
        <div class="row">
            @foreach (var car in Model)
            {
                <partial name="_CarPartial" model="@car" />
            }
        </div>
    }
}